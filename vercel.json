{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.x",
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    { "src": "/favicon.ico", "status": 204 },
    { "src": "/(.*)", "dest": "/app.py" }
  ]
}